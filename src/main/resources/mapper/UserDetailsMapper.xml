<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.security.config.security.UserDetailsMapper">

    <select id="getByUid">
        SELECT iuser, user_id, password, role
        FROM t_user
        WHERE user_id = #{user_id}
    </select>

</mapper>